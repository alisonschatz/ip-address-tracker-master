{
  "name": "ip-address-tracker",
  "version": 2,
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "GENERATE_SOURCEMAP=false npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}